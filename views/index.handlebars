<h2>Welcome back!</h2>

<h3>Current Bulletin Board</h3>
<table class="table table-hover">
    {{#each bulletins}}
    <tr class="row">
        <td>{{item.name}}</td>
        <td>{{quantity}}</td>
        <td class="{{type}}">{{type}}</td>
        <td>{{message}}</td>
        <td><a href="/profile/{{user.id}}">{{user.first_name}}</a></td>
    </tr>
    {{/each}}
    
</table>

<div class="bottom nav">
    <div class="footer inline-list">
        <ul>
            <li>About</li>
            <li>
                <a href="/help">Help</a>
            </li>
        </ul>
    </div>
</div>
